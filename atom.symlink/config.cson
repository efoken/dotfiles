"*":
  "color-picker":
    abbreviateValues: true
    automaticReplace: true
    preferredFormat: "HEX"
  core:
    disabledPackages: [
      "metrics"
      "exception-reporting"
      "background-tips"
    ]
    packagesWithKeymapsDisabled: []
    themes: [
      "atom-dark-ui"
      "sunburst"
    ]
  editor:
    fontFamily: "Hack"
    fontSize: 12
    invisibles: {}
    lineHeight: 1.3
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapHangingIndent: 4
    zoomFontWhenCtrlScrolling: false
  "file-icons": {}
  minimap: {}
  "release-notes":
    viewedVersion: "f0bd3b1"
  "split-diff":
    ignoreWhitespace: true
    syncHorizontalScroll: true
  welcome:
    showOnStartup: false
  whitespace: {}
