[user]
	name = AUTHOR_NAME
	email = AUTHOR_EMAIL
[credential]
	helper = GIT_CREDENTIAL_HELPER
[alias]
	count = !git shortlog -sn
	rank-contributors = !$ZSH/bin/git-rank-contributors
	wtf = !$ZSH/bin/git-wtf
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[core]
	excludesfile = ~/.gitignore
	editor = vim
[apply]
	whitespace = nowarn
[difftool]
	prompt = false
[mergetool]
	keepBackup = false
[push]
	default = simple
